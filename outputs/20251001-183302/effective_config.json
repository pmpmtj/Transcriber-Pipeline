{
  "model": "gpt-4o-transcribe",
  "response_format": "json",
  "prompt": "",
  "parallel_requests": 3,
  "max_retries": 3,
  "backoff_base_ms": 800,
  "max_file_mb": 25,
  "target_chunk_mb": 16,
  "max_chunk_secs": 900,
  "overlap_secs": 3.0,
  "reencode": {
    "enabled": true,
    "codec": "aac",
    "bitrate_kbps": 64,
    "channels": 1,
    "sample_rate": 16000
  },
  "segmenter": "fixed",
  "silence": {
    "min_silence_db": -35,
    "min_silence_dur": 0.6
  },
  "outputs": {
    "write_txt": true,
    "write_json": true,
    "write_srt": true,
    "write_vtt": false
  },
  "input_audio": "",
  "work_dir": "outputs"
}